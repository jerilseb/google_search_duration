{
  "manifest_version": 3,
  "name": "Google Search Duration",
  "description": "Easily change the duration of Google search",
  "version": "1.2",
  "icons": {
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}